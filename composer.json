{
    "name": "supseven/redirects-updater",
    "description": "Easily replace the source_host of existing TYPO3 redirects",
    "type": "typo3-cms-extension",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Supseven\\RedirectsUpdater\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "redirects_updater"
        }
    }
}
